{
  "name": "carrot-user-profiles",
  "version": "1.0.0",
  "description": "PostgreSQL-based user profile management system with Firebase Authentication integration",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "db:init": "psql $DATABASE_URL -f migrations/001_create_user_profiles.sql",
    "db:schema": "curl http://localhost:5000/api/db/schema"
